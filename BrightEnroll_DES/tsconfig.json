{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "allowJs": true,
    "checkJs": false,
    "types": []
  },
  "exclude": [
    "**/*.razor",
    "**/*.cs",
    "node_modules",
    "bin",
    "obj"
  ],
  "include": [
    "wwwroot/js/**/*.js"
  ]
}

